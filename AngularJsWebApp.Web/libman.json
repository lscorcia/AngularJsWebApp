{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.1.3",
      "destination": "lib/bootstrap"
    },
    {
      "library": "jquery@3.3.1",
      "destination": "lib/jquery",
      "files": [
        "jquery.js",
        "jquery.min.js",
        "jquery.min.map"
      ]
    },
    {
      "library": "angular.js@1.7.2",
      "destination": "lib/angular",
      "files": [
        "angular.js",
        "angular.min.js",
        "angular.min.js.map",
        "angular-mocks.js",
        "angular-route.js",
        "angular-route.min.js",
        "angular-route.min.js.map",
        "i18n/angular-locale_it-it.js"
      ]
    },
    {
      "library": "angular-local-storage@0.7.1",
      "destination": "lib/angular",
      "files": [
        "angular-local-storage.js",
        "angular-local-storage.min.js"
      ]
    },
    {
      "library": "angular-loading-bar@0.9.0",
      "destination": "lib/loading-bar"
    }
  ]
}